          <stringProp name="BeanShellSampler.query">import org.apache.jmeter.protocol.jdbc.config.DataSourceElement;
import java.sql.*;

BufferedReader br = null;
String line = &quot;&quot;;
br = new BufferedReader(new FileReader(&quot;updateQueryList.sql&quot;));

static final String JDBC_DRIVER = &quot;com.flipkart.polyglot.jdbc.PolyglotDriver&quot;;  
static final String DB_URL = &quot;jdbc:polyglot://txnstore:ekart-tds-0001.nm.flipkart.com:15991@archivalstore:172.17.94.213:2181:hbase/shipment/shipment?dataStoreType=txnl&quot;;

static final String USER = &quot;&quot;;
static final String PASS = &quot;&quot;;

Connection conn = null;
Class.forName(&quot;com.flipkart.polyglot.jdbc.PolyglotDriver&quot;);

conn = DriverManager.getConnection(DB_URL,USER,PASS);

System.out.println(&quot;Connection established!&quot;);

Statement st = conn.createStatement();
while ((line = br.readLine()) != null) {
	System.out.println(line);
 	try{
 		st.executeUpdate(line);
 	}
 	catch(Exception e){
 		System.out.println(e);
 	}

}
st.close();
conn.close();
</stringProp>
