			return b.toString();
		case &quot;VARCHAR&quot;:
        	  	String str = ${PREFIX_STRING} + randomString(length - 4).toString();
        	  	stmt.setString(place, str);
            	return (str.toString());
        	case &quot;TINYINT&quot;:
        		Integer b = rand.nextInt(${TINYINT_RANGE});
        	  	stmt.setInt(place, b);
            	return b.toString();
        	case &quot;BIT&quot;:
        	  	Integer b = rand.nextInt(${BIT_RANGE});
        	  	stmt.setInt(place, b);
