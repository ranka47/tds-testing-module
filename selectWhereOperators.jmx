				}
				try
				{
					stmt.executeUpdate();
				}
				catch (SQLException e){
					System.out.println(&quot;Error: &quot; + e.getMessage());
					IsSuccess=false;
				}
			}
		}
	}
}
catch(Exception e){
	IsSuccess=false;
	System.out.println(e);
}
System.out.println(&quot;--------------------\nDelete Phase Completed\n--------------------\n&quot;);	

stmt.close();
conn.close();</stringProp>
