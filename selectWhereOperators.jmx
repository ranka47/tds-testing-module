	for(int i=0; i&lt;colDataTypes.size(); i++){
		String colDataType = colDataTypes.get(i);
		String colName = colNames.get(i);
		switch(colDataType){
			case &quot;INT&quot;:
				txnl = txnl.concat(colName + &quot;!=&quot; + INT_NEQ_VALUE);
