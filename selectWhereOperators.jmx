String DATETIME_NEQ_VALUE = &quot;1000-01-01 00:00:00&quot;;

class query{
	String queryTxnl;
    	String querySec;
    	public query(String txnl, String sec){
     	queryTxnl = txnl;
        	querySec = sec;
