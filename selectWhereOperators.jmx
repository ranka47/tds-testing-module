int BIGINT_NEQ_VALUE = -${BIGINT_RANGE};
int INT_NEQ_VALUE = -${INT_RANGE};
int TINYINT_NEQ_VALUE = -${TINYINT_RANGE};
int BIT_NEQ_VALUE = -${BIT_RANGE};
